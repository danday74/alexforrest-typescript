    "karma": "^2.0.4",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-super-dots-reporter": "^0.2.0",
    "nib": "^1.1.2",
    "node-cmd": "^3.0.0",
