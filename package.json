    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@angular/router-deprecated": "^2.0.0-rc.2",
    "bootstrap": "^4.1.2",
    "core-js": "^2.4.1",
    "lodash": "^4.16.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "6.2.2",
    "systemjs": "^0.21.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "browser-sync": "^2.17.5",
    "chalk": "^2.4.1",
